{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "moduleResolution": "nodenext",
    "noEmit": true,
    "skipLibCheck": true, // "@fluentui/*" has internal conflicts on their types
    "strict": true,
    "target": "ESNext"
  },
  "exclude": ["**/*.spec.*", "**/*.test.*"]
}
